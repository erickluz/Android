		txtB = (EditText) findViewById(R.id.txtB);
		
	}

	
	public void btnOk(View v){
		
		Double a = Double.parseDouble(txtA.getText().toString());
		Double b = Double.parseDouble(txtB.getText().toString());
		
		
	}
	
}
